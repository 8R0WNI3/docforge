#!/bin/bash
source $(dirname "${0}")/common

sed -i "s/dev-\?[a-zA-Z0-9]*/dev-$(id -u -n)/g" $(dirname $0)/../VERSION
VERSION=$(cat $(dirname $0)/../VERSION)

echo "# Generated from hacks/templates/images.yaml.tpl" | cat - $(dirname $0)/templates/images.yaml.tpl | sed  "s/__VERSION_TPL__/${VERSION}/g" >${DEV_DIR}/images.yaml
